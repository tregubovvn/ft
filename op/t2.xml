<formulas>
    <formula name="Коэффициент использования пробега (первая формула)">
        <latex> β = Lг / Lсс </latex>
        <symbols>
            <symbol char="Lг" name="Пробег автомобиля с грузом, км" />
            <symbol char="Lсс" name="Общий (среднесуточный) пробег автомобиля, км" />
        </symbols>
    </formula>
    <formula name="Коэффициент использования пробега (вторая формула)">
        <latex> β = Lг / (Lг + Lх + L0) </latex>
        <symbols>
            <symbol char="Lг" name="Пробег автомобиля с грузом, км" />
            <symbol char="Lх" name="Пробег автомобиля без груза, км" />
            <symbol char="L0" name="Нулевой пробег автомобиля, км" />
        </symbols>
    </formula>
</formulas>